{
    "name": "open-health/mis",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["open", "health", "laravel", "framework"],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.4",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.10.0",
        "kirkbushell/eloquence": "^12.0.1",
        "laravel/fortify": "^1.33.0",
        "laravel/framework": "^12.43.1",
        "laravel/jetstream": "^5.4.0",
        "laravel/tinker": "^2.10.2",
        "livewire/livewire": "^3.7.2",
        "spatie/laravel-permission": "^6.24.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6.1",
        "fakerphp/faker": "^1.24.1",
        "fruitcake/laravel-debugbar": "^4.0.5",
        "laravel/pint": "^1.26.0",
        "laravel/sail": "^1.51.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "phpunit/phpunit": "^12.5.4",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Migrations\\Install\\": "database/migrations/install/",
            "Database\\Migrations\\Update\\": "database/migrations/update/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "fruitcake/laravel-debugbar"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
